<?php

class Employee
{
    public $name;
    public $age;
    public $salary;
}

//Первый объект
$employee1 = new Employee;
$employee1->name = 'john';
$employee1->age = 25;
$employee1->salary = 1000;

//Второй объект
$employee2 = new Employee;
$employee2->name = 'eric';
$employee2->age = 26;
$employee2->salary = 2000;

//Сумма зарплат юзеров
echo $employee2->salary + $employee1->salary;

//Общий возраст юзеров
echo $employee2->age + $employee1->age;
